    "metalsmith-concat": "^3.0.3",
    "metalsmith-define": "^1.0.0",
    "metalsmith-layouts": "^1.4.2",
    "metalsmith-markdown": "^0.2.1",
    "metalsmith-pagination": "^1.4.0",
    "metalsmith-permalinks": "^0.4.0",
    "metalsmith-sass": "^1.3.0",
    "metalsmith-serve": "0.0.4",
    "metalsmith-static": "0.0.5",
    "metalsmith-uglify": "^1.2.1",
    "metalsmith-updated": "^0.1.3",
    "metalsmith-watch": "^1.0.1",
    "moment": "^2.6.0",
    "onchange": "^3.2.1",
    "surge": "^0.18.0",
    "travis-ci": "^2.1.1"
  },
  "scripts": {
    "build-html": "node_modules/metalsmith/bin/metalsmith",
    "build": "npm run build-html",
